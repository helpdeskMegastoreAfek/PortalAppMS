{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "settings": {
    "react": { "version": "detect" }
  },
  "parserOptions": {
    "ecmaFeatures": { "jsx": true }
  }
}
